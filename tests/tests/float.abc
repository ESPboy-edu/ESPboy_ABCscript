float x;
float y;

void main()
{
    $debug_break();
    
    x = 1.5;
    y = 1.5;
    $assert(x == y);
    $assert(int(x) == 1);
    $assert(int(y) == 1);
    y = 2;
   
    $assert(y == 2);
    $assert(y == 2.0);
    $assert(y == 2e0);
    $assert(y == 2.0e0);
    $assert(y == 20e-1);
    $assert(y == 0.2e1);
    $assert(y == 0.2e+1);
    $assert(!(x == y));
    $assert(x != y);
    $assert(x < y);
    $assert(x <= y);
    $assert(y > x);
    $assert(y >= x);
    $assert(int(y) == 2);
    y = -y;
    $assert(y == -2);
    $assert(y == -2.0);
    $assert(y == -2e0);
    $assert(y == -2.0e0);
    $assert(y < 0);
    $assert(x != y);
    $assert(x > y);
    $assert(x >= y);
    $assert(y < x);
    $assert(y <= x);
    $assert(int(y) == -2);
    
    $assert(x + y < 0);
    $assert(x + y == -0.5);
    $assert(x + y == -1.0 / 2);
    $assert(x + y == 1.0 / -2);
    $assert(x + y == -1 / 2.0);
    $assert(x + y == 1 / -2.0);
    $assert(x + y != 1 / 2);
    
    $assert(x - y == 3.5);
    $assert(x * y == -3);
    $assert(x / y == -0.75);
    
    $assert(x == 1.5);
    $assert(++x == 2.5);
    $assert(x == 2.5);
    
    $assert(x == 2.5);
    $assert(x++ == 2.5);
    $assert(x == 3.5);
    
    $assert(x == 3.5);
    $assert(--x == 2.5);
    $assert(x == 2.5);
    
    $assert(x == 2.5);
    $assert(x-- == 2.5);
    $assert(x == 1.5);
        
    $debug_break();
}
