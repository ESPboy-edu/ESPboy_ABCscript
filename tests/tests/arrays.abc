void main()
{
    $debug_break();
    
    u16[4] n = { 1, 2, 3, 4 };
    $assert(n[0] == 1);
    $assert(n[1] == 2);
    $assert(n[2] == 3);
    $assert(n[3] == 4);
    n = { 5, 6, 7, 8 };
    $assert(n[0] == 5);
    $assert(n[1] == 6);
    $assert(n[2] == 7);
    $assert(n[3] == 8);
    n = { 0x10001, 0x10002, 0x10003, 0x10004 };
    $assert(n[0] == 1);
    $assert(n[1] == 2);
    $assert(n[2] == 3);
    $assert(n[3] == 4);
    
    $debug_break();
}
