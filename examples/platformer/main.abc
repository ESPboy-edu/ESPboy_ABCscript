import map;
import update;
import render;
import sounds;
import vars;

void setup()
{
    $set_frame_rate(50);
    current_map = 0;
    state = STATE_INTRO;
}

void loop()
{
    while(!$next_frame())
        ;
    
    update();
    render();
    
    $display();
}

void main()
{
    setup();
    while(true)
        loop();
}
