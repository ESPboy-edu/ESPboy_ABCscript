.global x 2
    
    push 30
    sys  set_frame_rate
    
frame:
    sys  next_frame
    bz   frame
    
    getg2 x
    push 0
    push 0
    push 16
    push 16
    push 1
    sys  draw_filled_rect
    
    call increment_x
    
    sys  display
    jmp  frame

increment_x:
    getg2 x
    push 1
    push 0
    add2
    setg2 x
    ret